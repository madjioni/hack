<!doctype html>
<html lang="en">
	<head>
		<meta charset="UTF-8">

		<title>{title}</title>

		<!-- <link rel="shortcut icon" href="{CONFIG_WEBSITE}/{CONFIG_IMG_DIR}/favicon.png" type="image/x-icon">
		<link rel="icon" href="{CONFIG_WEBSITE}/{CONFIG_IMG_DIR}/favicon.png" type="image/x-icon"> -->
		<link rel="stylesheet" href="{CONFIG_CSS_DIR}/[block css]style.css[end]">

		<script type="text/javascript" src="{CONFIG_JS_DIR}/jquery.js"></script>
	</head>
	
	<body>
