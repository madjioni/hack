    <div id="home" class="static-header light">
        <div class="text-heading">
            <h1>Registracija <span class="highlight">poslodavca</span></h1>
        </div>

  <a id="showHere"></a>

        <div class="container">
            <div class="row">
                <div class="col-lg-6 col-lg-offset-3 col-md-8 col-md-offset-2 col-sm-10 col-sm-offset-1 col-xs-12">
                    <form class="form form-register dark" id="registration" method="post" name="form_rfe" action="/Registration/t/e" method="POST">
                        <div class="form-group">
                            <label for="firstname" class="col-sm-3 col-xs-12 control-label">Ime</label>
                            <div class="col-sm-9 col-xs-12">
                                <input type="text" name="firstname_rfe" class="form-control required" id="firstname" placeholder="">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="lastname" class="col-sm-3 col-xs-12 control-label">Prezime</label>
                            <div class="col-sm-9 col-xs-12">
                                <input type="text" name="lastname_rfe" class="form-control required" id="lastname" placeholder="">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="email" class="col-sm-3 col-xs-12 control-label">Email</label>
                            <div class="col-sm-9 col-xs-12">
                                <input type="email" name="email_rfe" class="form-control required email" id="email" placeholder="">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="email" class="col-sm-3 col-xs-12 control-label">Å ifra</label>
                            <div class="col-sm-9 col-xs-12">
                                <input type="password" name="pass_rfe" class="form-control" id="password" placeholder="">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="phone" class="col-sm-3 col-xs-12 control-label">Telefon</label>
                            <div class="col-sm-9 col-xs-12">
                                <input type="text" name="phone_rfe" class="form-control required phone" id="phone" placeholder="">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="location" class="col-sm-3 col-xs-12 control-label">Lokacija</label>
                            <div class="col-sm-9 col-xs-12">
                                <input type="text" name="location_rfe" class="form-control" id="location" placeholder="">
                            </div>
                        </div>
                        <div class="form-group">
                            <label for="description" class="col-sm-3 col-xs-12 control-label">Opis</label>
                            <div class="col-sm-9 col-xs-12">
                                <textarea type="text" class="form-control required" name="description_rfe" id="description" placeholder=""></textarea>
                            </div>
                        </div>
                        <button type="submit" class="btn btn-primary btn-lg btn-block">Regisrtruj se</button>
                    </form>
                </div>
            </div>
        </div>
    </div>


