<div>
[if editable]
    [if worker]
    <form>
        <input type="text" name="fn" value="{user.firstname}"><br>
        <input type="text" name="ln" value="{user.lastname}"><br>
        <input type="text" nme="loc" value="{user.location}"><br>
        <input type="text" name="ph" value="{user.phone}"><br>
        <input type="text" name="mail" value="{user.mail}"><br>
        <button type="submit">Izmeni</button>
    </form>

    {poslovi}

    [end]




    [if employer]
    <form>
        <input type="text" name="fn" value="{user.firstname}"><br>
        <input type="text" name="ln" value="{user.lastname}"><br>
        <input type="text" nme="loc" value="{user.location}"><br>
        <input type="text" name="ph" value="{user.phone}"><br>
        <input type="text" name="mail" value="{user.mail}"><br>
        <button type="submit">Izmeni</button>
    </form>

    {poslovi}

    [end]

[else]
    [if worker]
    <p>
        IME:{user.firstname} <br>
        PREZIME: {user.lastname} <br>
        Lokacija: {user.location} <br>   
        e-mail: {user.mail} <br>
        Pol: {user.gender} <br>
        Godine: {user.age} <br>
        Tel: {user.phone}
        </p>

    [if poslovi|count]
        [foreach posao in poslovi]
            <p>Naziv: {posao.idjob.title} <br>
            Rate: {posao.idjob.ewrate} <br>
            Comm: {posao.idjob.ewcomm}</p>
        [end]
    [end]
        
    [end]

    [if employer]
    <p>
        IME:{user.firstname} <br>
        PREZIME: {user.lastname} <br>
        Lokacija: {user.location} <br>   
        e-mail: {user.mail} <br>
        Opis: {user.description} <br>
        Tel: {user.phone}
    </p>

    [if poslovi|count]
        [foreach posao in poslovi]
            <p>Naziv: {posao.idjob.title} <br>
            Rate: {posao.idjob.werate} <br>
            Comm: {posao.idjob.wecomm}</p>
        [end]
    [end]

    [end]
    
[end]
   

</div>

